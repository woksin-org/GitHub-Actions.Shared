{
    "name": "@woksin/github-actions.shared.git",
    "version": "1.1.3",
    "description": "Shared package for interacting with git",
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/woksin-org/Github-Actions.Shared"
    },
    "bugs": {
        "url": "https://github.com/woksin-org/Github-Actions.Shared/issues"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "files": [
        "Distribution",
        "*.ts"
    ],
    "main": "Distribution/index.js",
    "types": "Distribution/index.d.ts",
    "scripts": {
        "prebuild": "yarn clean",
        "postbuild": "yarn lint",
        "build": "tsc -b tsconfig.json ",
        "clean": "tsc -b --clean ",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "lint": "eslint \"**/*.ts\"",
        "test": "mocha",
        "ci": "yarn build && yarn test"
    },
    "dependencies": {
        "@actions/exec": "^1.1.1",
        "@woksin/github-actions.shared.logging": "^1.1.3"
    }
}